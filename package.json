{
  "name": "@speakingsoftware/anonimizer",
  "version": "1.2.0",
  "description": "A lib to anonimize text and later rehydrate derived text",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "repository": {
    "url": "https://github.com/FiddlyDigital/Anonimizer"
  },
  "author": "SpeakingSoftware / Philip Gannon",
  "license": "ISC",
  "keywords": [
    "anonimize",
    "sanitize",
    "prompt",
    "data",
    "data security",
    "text",
    "string"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "docs:generate": "typedoc --options ./typedoc.json",
    "docs:host": "http-server -p 3030 -c -1",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "test": "jest tests --verbose --config=./tests/jest.config.cjs"
  },
  "tsup": {
    "target": "es5"
  },
  "dependencies": {
    "compromise": "^14.9.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.68",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.1.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  }
}
